<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sharif.onlinebooking">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/book_image"
        android:label="@string/app_name"
        android:roundIcon="@drawable/book_image"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"   android:exported="false"/>
        <activity android:name=".Admin" android:exported="false"/>
        <activity android:name=".Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Operation" android:exported="false"/>
        <activity android:name=".Deletion" android:exported="false"/>
        <activity android:name=".Update" android:exported="false"/>
        <activity android:name=".UpdatOperation" android:exported="false" />
        <activity android:name=".AdminLogin" android:exported="false"></activity>
        <receiver android:name=".NotificationService"/>
    </application>

</manifest>